    "query": {
        "from": {
            "table": "Sales\\Receipts\\Products\\Code\\Tables\\Products",
            "alias": "srp",
            "name": "#__sales_receipts_products"
        },
        "select": [
            "srp.id",
            "srp.product_id",
            "pp.title AS product_id_title",
            "srp.receipt_id",
            "rr.title AS receipt_id_title",
            "srp.quantity",
            "srp.unit_price",
            "srp.created_by",
            "srp.date_created",
            "srp.modified_by",
            "srp.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Products\\Products\\Code\\Tables\\Products",
                "alias": "pp",
                "name": "#__products_products",
                "on": "srp.product_id = pp.id"
            },
            {
                "table": "Recipes\\Recipes\\Code\\Tables\\Recipes",
                "alias": "rr",
                "name": "#__recipes_recipes",
                "on": "srp.receipt_id = rr.id"
            }
        ]
    }
